<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent"
    tools:context=".OpenActivity">
    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@color/nevi_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/back"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_margin="5dp"
            app:srcCompat="@drawable/back"></ImageView>

        <TextView
            android:layout_width="185dp"
            android:layout_height="35dp"
            android:layout_marginStart="50dp"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:text="SBC/VJ/NT Category"
            android:fontFamily="@font/lora"
            android:textColor="@color/white"
            android:textSize="18dp">
        </TextView>

        <ImageView
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginStart="40dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp"
            app:srcCompat="@drawable/documents">
        </ImageView>
    </LinearLayout>
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">
        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:orientation="vertical"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout3">
            <TextView
                android:layout_marginTop="20dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textColor="@color/white"
                android:background="#F2150E65"
                android:fontFamily="@font/lora"
                android:text="Eligibility"
                android:textSize="20sp"
                android:gravity="center">
            </TextView>
            <TextView
                android:layout_marginTop="20dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#0F018786"
                android:padding="10dp"
                android:fontFamily="@font/lora"
                android:text="40% marks in Physics, Mathematics and Chemistry / Biology / Vocational subjects in HSC and appeared in MHT-CET 2023 or JEE Mains (Paper 1) 2023"
                android:textColor="@color/black"
                android:textSize="16sp">
            </TextView>

            <TextView
                android:layout_marginTop="20dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textColor="@color/white"
                android:background="#F2150E65"
                android:text="Documents"
                android:fontFamily="@font/lora"
                android:textSize="20sp"
                android:gravity="center">
            </TextView>
            <TextView
                android:layout_marginTop="20dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:background="#0F018786"
                android:padding="10dp"
                android:fontFamily="@font/lora"
                android:text="SSC mark sheet"
                android:textColor="@color/black"
                android:textSize="18sp">
            </TextView>
            <TextView
                android:layout_marginTop="2dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:background="#0F018786"
                android:padding="10dp"
                android:text="HSC mark sheet"
                android:fontFamily="@font/lora"
                android:textColor="@color/black"
                android:textSize="18sp">
            </TextView>
            <TextView
                android:layout_marginTop="2dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:background="#0F018786"
                android:padding="10dp"
                android:fontFamily="@font/lora"
                android:text="MHT-CET / JEE mark sheet"
                android:textColor="@color/black"
                android:textSize="18sp">
            </TextView>
            <TextView
                android:layout_marginTop="2dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:background="#0F018786"
                android:fontFamily="@font/lora"
                android:padding="10dp"
                android:text="Leaving certificate"
                android:textColor="@color/black"
                android:textSize="18sp">
            </TextView>
            <TextView
                android:layout_marginTop="2dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:background="#0F018786"
                android:padding="10dp"
                android:fontFamily="@font/lora"
                android:text="Nationality certificate"
                android:textColor="@color/black"
                android:textSize="18sp">
            </TextView>
            <TextView
                android:layout_marginTop="2dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:background="#0F018786"
                android:padding="10dp"
                android:fontFamily="@font/lora"
                android:text="Domicile certificate"
                android:textColor="@color/black"
                android:textSize="18sp">
            </TextView>
            <TextView
                android:layout_marginTop="2dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:background="#0F018786"
                android:padding="10dp"
                android:fontFamily="@font/lora"
                android:text="Caste Certificate"
                android:textColor="@color/black"
                android:textSize="18sp">
            </TextView>
            <TextView
                android:layout_marginTop="2dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:background="#0F018786"
                android:padding="10dp"
                android:fontFamily="@font/lora"
                android:text="Cast Validity Certificate"
                android:textColor="@color/black"
                android:textSize="18sp">
            </TextView>
            <TextView
                android:layout_marginTop="2dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:background="#0F018786"
                android:padding="10dp"
                android:fontFamily="@font/lora"
                android:text="Non Creme-Layer Certificate (valid upto march 2024)"
                android:textColor="@color/black"
                android:textSize="18sp">
            </TextView>
            <TextView
                android:layout_marginTop="20dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textColor="@color/white"
                android:background="#F2150E65"
                android:fontFamily="@font/lora"
                android:text="Income Certificate"
                android:textSize="20sp"
                android:gravity="center">
            </TextView>
            <TextView
                android:layout_marginTop="20dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:background="#0F018786"
                android:padding="10dp"
                android:fontFamily="@font/lora"
                android:text="Parent Income certificate of 2022-23 (less than 8 lakh)"
                android:textColor="@color/black"
                android:textSize="18sp">
            </TextView>

        </LinearLayout>

    </androidx.core.widget.NestedScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>